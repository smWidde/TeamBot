<Window x:Class="TeamProject_.UsersWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TeamProject_"
        mc:Ignorable="d"
        Title="UsersWindow" Height="450" Width="480.328">
    <Grid>
        <ListBox Name="lsbox" HorizontalAlignment="Left" Height="337" Margin="27,10,0,0" VerticalAlignment="Top" Width="423">
            <ListBox.ItemContainerStyle>
                <Style TargetType="ListBoxItem">
                    <Setter Property="IsSelected" Value="{Binding IsSelected}" />
                </Style>
            </ListBox.ItemContainerStyle>
        </ListBox>
        <Button Content="Перейти к пользователю" HorizontalAlignment="Left" Margin="163,369,0,0" VerticalAlignment="Top" Width="157" Height="40" Click="Button_Click"/>

    </Grid>
</Window>
